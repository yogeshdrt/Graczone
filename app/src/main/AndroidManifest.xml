<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.graczone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Graczone">
        <activity android:name=".Phone_OTP"></activity>
        <activity
            android:name=".joining"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Wallet.withdraw"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Wallet.transactions"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Wallet.wallet"
            android:screenOrientation="portrait" />
        <activity
            android:name=".home"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Graczone.NoActionBar" />
        <activity
            android:name=".create_account"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>